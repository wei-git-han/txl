<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.css.base.log.dao.PerformanceDao">

	<!-- <select id="getProjectByUserName" resultType="com.css.log.entity.PerformanceEntity"> 
		select * from t_project_info where GCPM = #{0} </select> -->

	<select id="queryList" resultType="com.css.base.log.entity.SysLogPerformance">
		select * from T_Log_Performance where 1=1
		<!-- <if test="projectId != null "> and PROJECT_ID = #{projectId} </if> -->

		<if test="offset != null and limit != null">
			limit #{offset}, #{limit}
		</if>
	</select>

	<insert id="save" parameterType="com.css.base.log.entity.SysLogPerformance">
		insert into SYS_Log_Performance
		(
		"url",
		"per_id",
		"system_Code",
		"op_Name",
		"execute_Time",
		"use_Time",
		"method"
		)
		values
		(
		#{url},
		#{perId},
		#{systemCode},
		#{opName},
		#{executeTime},
		#{useTime},
		#{method}
		)
	</insert>

</mapper>