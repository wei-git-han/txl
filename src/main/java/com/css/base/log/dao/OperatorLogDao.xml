<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.css.base.log.dao.OperatorLogDao">


	<!-- <select id="getProjectByUserName" resultType="com.css.log.entity.OperatorLogEntity"> 
		select * from T_Log_Operator where GCPM = #{0} </select> -->

	<select id="queryList" resultType="com.css.base.log.entity.SysLogOperator">
		select * from T_Log_Operator where 1=1
		<!-- <if test="projectId != null "> and PROJECT_ID = #{projectId} </if> -->

		<if test="offset != null and limit != null">
			limit #{offset}, #{limit}
		</if>
	</select>
	<insert id="save" parameterType="com.css.base.log.entity.SysLogOperator">
		insert into SYS_Log_Operator
		(
		"user_Id",
		"op_Time",
		"system_Code",
		"data",
		"op_Name",
		"operator"
		)
		values
		(
		#{userId},
		#{opTime},
		#{systemCode},
		#{data},
		#{opName},
		#{operator}
		)
	</insert>
</mapper>